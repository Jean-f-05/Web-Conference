<nav>
    <nav id="navbar" class="navbar navbar-expand-md navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/webconference">WebConference</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse text-uppercase" id="navbarNav" >
            <ul class="navbar-nav ms-auto">
              <li class="nav-item">
                <a class="nav-link" href="/webconference/#about">About</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/webconference/#speakers">Speakers</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/webconference/#Sponsor">Sponsors</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/webconference/#contacts">Contacts</a>
              </li>
             <% if(!currentUser){ %>
              
              <li class="nav-item">
                <a class="nav-link" href="" data-bs-toggle="modal" data-bs-target="#loginModal">Login</a>
                
                  <!-- Modal -->
                  <form action="/authentication/login" method="POST" novalidate class="validated-form">
                    <div class="modal fade" id="loginModal" tabindex="-1" aria-labelledby="loginModalLabel" aria-hidden="true">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <h5 class="modal-title mx-auto" id="loginModalLabel">Admin login</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                          </div>
                          <div class="modal-body">
                            <div class="row mx-auto mb-3">
                                <input type="text" class="form-control" placeholder="username" aria-label="username" aria-describedby="basic-addon1" name="username" id="username" required>
                            </div>
                            <div class="row mx-auto mb-3">
                              <input type="password" class="form-control" placeholder="password" aria-label="Password" aria-describedby="basic-addon1" name="password" id="password" required>
                            </div>
                          </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-primary">Login</button>
                          </div>
                        </div>
                      </div>
                    </div>
                  </form>
                </li>
              <% } %>  
              

                  <% if(currentUser){ %>
               
                  <li class="nav-item">
                    <a class="nav-link" href="/authentication/logout">Logout</a>
                  </li>
                    <% } %>  

            </ul>
          </div>
        </div>
